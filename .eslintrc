---
"env": {
    "browser": true,
    "es6": true
}
rules:
  space-before-blocks: 2
  indent: 0
  brace-style: 2
  keyword-spacing: 2
  no-unused-expressions: 2
  eol-last: 2
  dot-notation: 2
  dot-location: [2, "property"]
  consistent-return: 2
  no-unused-vars: [1, "args": "none"]
  quotes: [2, "single", "avoid-escape"]
  no-shadow: 2
  no-multi-spaces: 2
  new-cap: 0
  no-underscore-dangle: 0
  no-debugger: 0
  no-use-before-define: 0
  semi: [2, "always"]
  strict: 0
  prefer-const: 2
  no-var: 2
  no-const-assign: 2
